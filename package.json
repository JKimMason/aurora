{
  "name": "aurora",
  "version": "0.0.0",
  "description": "",
  "main": "app/main/index.js",
  "scripts": {
    "build": "webpack --config app/renderer/renderer.config.js",
    "test": "./node_modules/.bin/jest",
    "test-on-ci": "xvfb-maybe ./node_modules/.bin/jest --runInBand",
    "test-watch": "./node_modules/.bin/jest --watch",
    "plop": "./node_modules/.bin/plop",
    "lint": "./node_modules/.bin/eslint lib app",
    "lint-fix": "./node_modules/.bin/eslint --fix lib app",
    "dev": "NODE_ENV=dev && npm run build && electron app/main/index.js",
    "dev-windows": "set NODE_ENV=dev && npm run build && electron app/main/index.js",
    "fmt": "./node_modules/.bin/prettier --write '!(coverage|scripts|.vscode|node_modules)/**/!(.compiled|build|dist)/*.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dist": "./node_modules/.bin/electron-packager . --out ./dist --overwrite"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.8",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.1",
    "devtron": "^1.4.0",
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^3.8.1",
    "plop": "^1.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.3.0",
    "tmp": "0.0.33",
    "webpack": "^3.0.0",
    "xvfb-maybe": "^0.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/dist/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/dist/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@react-mutate/core": "^0.4.4",
    "@react-mutate/loader": "^0.4.4",
    "animate-css-styled-components": "0.0.19",
    "draft-js": "^0.10.1",
    "fs-jetpack": "^1.2.0",
    "fuse.js": "^3.1.0",
    "immutable-props": "^1.1.0",
    "linklocal": "^2.8.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sqlite3": "^3.1.13",
    "styled-components": "^2.1.2",
    "uuid": "^3.1.0"
  },
  "build": {
    "appId": "com.flaque.penguin",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        "pkg",
        "dmg",
        "zip",
        "mas"
      ],
      "entitlements": "app/build/entitlements.mac.plist",
      "type": "distribution",
      "icon": "app/build/icon.icns"
    },
    "mas": {
      "entitlements": "build/entitlements.mas.plist",
      "type": "distribution",
      "category": "public.app-category.graphics-design"
    },
    "files": [
      "main.js",
      "node_modules/**/*",
      "app/**/*",
      "package.json",
      "embedded.provisionprofile"
    ]
  }
}
