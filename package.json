{
  "name": "electron-webpack-quick-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "db-down": "node node_modules/db-migrate/bin/db-migrate down -e $ENV --config config/database.json",
    "db-up": "node node_modules/db-migrate/bin/db-migrate up -e $ENV --config config/database.json",
    "dev-reset": "node node_modules/db-migrate/bin/db-migrate reset -e $ENV --config config/database.json",
    "db-create-config": "babel-node lib/io/sqlite/scripts/createDatabaseConfig.js",
    "db-migrate-create": "node node_modules/db-migrate/bin/db-migrate create $NAME -e dev --config config/database.json",
    "lint": "./node_modules/.bin/eslint src"
  },
  "dependencies": {
    "@react-mutate/core": "^0.4.4",
    "@react-mutate/loader": "^0.4.4",
    "axios": "^0.17.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "db-migrate": "^0.10.0-beta.24",
    "debounce": "^1.1.0",
    "draft-js": "^0.10.4",
    "knex": "^0.14.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "source-map-support": "^0.5.0",
    "styled-components": "^2.2.3"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "electron": "1.7.9",
    "electron-builder": "^19.42.1",
    "electron-webpack": "1.10.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.1.2",
    "webpack": "^3.8.1"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "enzyme-react-16-adapter-setup"
    ]
  }
}
